apiVersion: v1
kind: Service
metadata:
    labels:
        app.kubernetes.io/name: kube-state-metrics
        app.kubernetes.io/version: v1.8.0
    name: kube-state-metrics
    namespace: grafana
spec:
    clusterIP: None
    ports:
        - name: http-metrics
          port: 8080
          targetPort: http-metrics
        - name: telemetry
          port: 8081
          targetPort: telemetry
    selector:
        app.kubernetes.io/name: kube-state-metrics
